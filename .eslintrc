{
  "env": {
    "browser": true,
    "es6": true
  },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "prettier"],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "airbnb",
      "airbnb/hooks",
      "airbnb-typescript"],
    "parserOptions": {"project": "./tsconfig.json"},
